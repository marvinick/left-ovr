<!DOCTYPE html>
<html>
<head>
  <title>leftOvr</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">
</head>
<body>
  <div class="nav">
  <%= nav_bar :static => :top, :responsive => true, :brand => "leftOvr", :brand_link => root_path do %>
    <%= menu_group do %>
          <%= menu_item "Home", root_path %>
          <%= menu_divider %>
          <%= menu_item "Logout" %>
          <%= menu_item "Cities"%>
          <%= menu_item "News"%>
          <%= menu_item "About Us" %>
          <%= menu_item "Contact" %>
      <% end %>
    <% end %>
  </div>
<%= yield %>

</body>
</html>
