<div class="col-md-4">
  <%= form_tag '/login' do %>
  <div class="control-group">
    <%= label_tag :name %>
    <%= text_field_tag :name %>
  </div>
  <div class="control-group">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <br/>
    <%= submit_tag 'Login', class: 'btn btn-success' %>
  <% end %>
</div>
<br/>
<div class='col-md-4>'>
 <h5><%= link_to "Members", users_path %></h5>
</div>